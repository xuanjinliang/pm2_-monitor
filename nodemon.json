{
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules",
      "README.md",
      "package.json",
      ".editorconfig",
      ".eslintrc",
      ".gitignore",
      "favicon.ico",
      "testScript",
      "app.json"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "events": {
        "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json"
}
